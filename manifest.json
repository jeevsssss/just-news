{
  "manifest_version": 2,
  "name": "News Extension",
  "version": "1.0",
  "description": "A Chrome extension for displaying news articles",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "https://newsapi.org/*",
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/ 'unsafe-inline'; object-src 'self'"
}
